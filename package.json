{
  "name": "@zionix-web/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "env-cmd -f .env-cmdrc -e dev nx run zionix-main-host:serve",
    "start-zionix-main": "node tools/deployment/zionix-main.script.config.js",
    "lint": "nx run-many --target=lint",
    "lint:fix": "nx run-many --target=lint --fix",
    "eslint": "eslint --ext .js,.ts,.jsx,.tsx apps/",
    "prepare": "husky",
    "deploy:vercel": "node tools/dockerconfig/deploy-vercel.js",
    "deploy:main": "node tools/dockerconfig/deploy-vercel.js zionix-main-host",
    "deploy:admin": "node tools/dockerconfig/deploy-vercel.js adminApp",
    "docker:dev": "cd tools/dockerconfig && docker-compose up",
    "docker:prod": "cd tools/dockerconfig && docker-compose -f docker-compose.prod.yml up",
    "perf:audit": "lighthouse http://localhost:4200 --output=json --output-path=./lighthouse-report.json --only-categories=performance --chrome-flags=\"--headless\"",
    "perf:audit-admin": "lighthouse http://localhost:4200/adminApp/menu-setup --output=json --output-path=./lighthouse-admin-report.json --only-categories=performance --chrome-flags=\"--headless\"",
    "perf:analyze": "node -e \"const report = JSON.parse(require('fs').readFileSync('./lighthouse-report.json', 'utf8')); console.log('Performance Score:', report.categories.performance.score * 100); console.log('FCP:', report.audits['first-contentful-paint'].displayValue); console.log('LCP:', report.audits['largest-contentful-paint'].displayValue); console.log('TBT:', report.audits['total-blocking-time'].displayValue);\"",
    "perf:bundle-analyze": "nx build adminApp --analyze && echo 'Bundle analysis complete. Check dist folder for stats.'",
    "perf:monitor": "npm run perf:audit && npm run perf:analyze"
  },
  "private": true,
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fluent/bundle": "^0.19.1",
    "@fontsource/inter": "^5.2.8",
    "@tanstack/react-query": "^5.90.5",
    "antd": "^5.27.6",
    "axios": "^1.12.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "env-cmd": "^10.1.0",
    "framer-motion": "^12.23.24",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "react": "18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.3.1",
    "react-number-format": "^5.4.4",
    "react-router-dom": "6.11.2",
    "remixicon": "^4.7.0",
    "shortid": "^2.2.17",
    "styled-components": "^6.1.19",
    "tslib": "^2.3.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@module-federation/enhanced": "^0.18.0",
    "@nx/eslint": "22.0.2",
    "@nx/eslint-plugin": "22.0.2",
    "@nx/jest": "22.0.2",
    "@nx/js": "22.0.2",
    "@nx/module-federation": "22.0.2",
    "@nx/react": "22.0.2",
    "@nx/rspack": "22.0.2",
    "@nx/web": "22.0.2",
    "@nx/workspace": "22.0.2",
    "@rspack/cli": "^1.5.8",
    "@rspack/core": "^1.5.8",
    "@rspack/dev-server": "1.1.4",
    "@rspack/plugin-minify": "^0.7.5",
    "@rspack/plugin-react-refresh": "^1.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "0.2.39",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/node": "^20.0.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "babel-jest": "^30.0.2",
    "css-loader": "^7.1.2",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "file-loader": "^6.2.0",
